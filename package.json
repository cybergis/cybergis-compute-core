  "bugs": {
    "url": "https://github.com/cybergis/job-supervisor/issues"
  },
  "homepage": "https://github.com/cybergis/job-supervisor#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "better-sqlite3": "^7.5.1",
    "body-parser": "^1.20.1",
    "commander": "^6.2.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.2.0",
    "jsonschema": "^1.2.6",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-ssh": "^10.0.2",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.34",
    "util": "^0.12.3",
    "validator": "^13.7.0"
  },
